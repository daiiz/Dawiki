{
  "name": "dawiki",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:daiz713/Dawiki.git",
  "author": "daiiz <da.iz.mg@gmail.com>",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": "7.6.0"
  },
  "scripts": {
    "postinstall": "yarn run build",
    "start": "node app.js",
    "start:dev": "node-dev app.js",
    "build": "./node_modules/.bin/run-s build:**",
    "build:browserify": "browserify dawiki-wiki/src/index.js -o dawiki-wiki/dist/bundle.js",
    "build:bower": "cd ./dawiki-wiki && ../node_modules/bower/bin/bower install",
    "test": "./node_modules/.bin/run-s test:**",
    "test:ava": "ava test/**/*.js",
    "test:standard": "standard ./dawiki-wiki/src/**/**.js ./dawiki-wiki/src/*.js ./client/*.js ./server/*.js ./test/**/**.js --fix",
    "watch": "watchify dawiki-wiki/src/index.js -o dawiki-wiki/dist/bundle.js -v"
  },
  "dependencies": {
    "@google-cloud/datastore": "^1.0.7",
    "argv": "^0.0.2",
    "body-parser": "^1.17.2",
    "bower": "^1.8.0",
    "browserify": "^14.4.0",
    "diff": "^3.3.0",
    "ejs": "^2.5.7",
    "express": "^4.15.3",
    "jquery": "^3.2.1",
    "npm-run-all": "^4.0.2",
    "socket.io": "^2.0.3",
    "socket.io-client": "^2.0.3",
    "socket.io-request": "^0.3.1",
    "superagent": "^3.5.2",
    "unirest": "^0.5.1"
  },
  "devDependencies": {
    "ava": "^0.20.0",
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "standard": "^10.0.2",
    "watchify": "^3.9.0"
  }
}
